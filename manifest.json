{
  "manifest_version": 2,
  "name": "HenHacks 2026: Manga Extractor",
  "version": "1.0",
  "description": "Extracts text from manga images using local Python server.",
  "icons": {
    "48": "icons/book.png"
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}